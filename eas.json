{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentialsContext": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://campusvibe-backend-q5os.onrender.com",
        "ORG_GRADLE_PROJECT_org_gradle_jvmargs": "-Xmx2048m -XX:MaxMetaspaceSize=512m"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://campusvibe-backend-q5os.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
